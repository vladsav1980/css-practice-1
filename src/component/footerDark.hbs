<footer>

    <div class="footer">


        {{#with footer}}
        <div class="">
            <span class="footer_copyright">{{copyright}}</span>
        </div>

        <div class="footer_nav">
            {{#each pages}}
            <a href="{{this.path}}" class="nav_link">{{this.name}}</a>
            {{/each}}
        </div>
        {{/with}}

    </div>
</footer>